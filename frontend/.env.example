# MusicMoodBot Frontend Configuration
# =====================================
# Copy this file to .env and update values as needed

# API Configuration
API_BASE_URL=http://localhost:8000
API_TIMEOUT=30.0
API_MAX_RETRIES=3
API_DEBUG=false

# Authentication
# Token storage mode: "flet" (recommended), "file", or "memory"
AUTH_STORAGE_MODE=flet

# App Settings
APP_NAME=MusicMoodBot
APP_VERSION=2.0.0
DEBUG_MODE=false
