# Music Mood Prediction API Configuration

# Database
DATABASE_PATH=music.db

# API Settings
API_HOST=0.0.0.0
API_PORT=8000
API_DEBUG=false

# Mood Engine
MOOD_ENGINE_AUTO_FIT=true
MOOD_ENGINE_ADD_DEBUG_COLS=false
MOOD_ENGINE_FORCE_FIT=false

# Search Engine
SEARCH_TOP_K=10
SEARCH_MIN_SCORE=0.0

# Preference Model
PREFERENCE_MIN_SAMPLES=3
PREFERENCE_BATCH_SIZE=32

# CORS Settings
CORS_ORIGINS=*
CORS_CREDENTIALS=true
CORS_METHODS=GET,POST,PUT,DELETE
CORS_HEADERS=*
